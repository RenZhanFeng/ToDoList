	    -ms-flex-pack: center;
	        justify-content: center;
	position: fixed;
	bottom: 20px;
	left: 0;
	right: 0;
	margin: 0 auto;
	width: 50px;
	height: 50px;
	border-radius: 50px;
	background: #deeff5;
	-webkit-box-shadow: -1px 1px 5px 2px rgba(0, 0, 0, .1), -1px 1px 1px 0 rgb(255, 255, 255)inset;
	        box-shadow: -1px 1px 5px 2px rgba(0, 0, 0, .1), -1px 1px 1px 0 rgb(255, 255, 255)inset;
}
.icon-jiahao {
	font-size: 30px;
	color: #add8e6;
}
.create-content {
	position: fixed;
	bottom: 95px;
	left: 20px;
	right: 20px;
	opacity: 0;
	-webkit-transition: all .3s;
	-o-transition: all .3s;
	transition: all .3s;
	-webkit-transform: scale(0) translateY(300%);
	    -ms-transform: scale(0) translateY(300%);
	        transform: scale(0) translateY(300%);
}
.create-show {
	opacity: 1;
	-webkit-transform: scale(1) translateY(0);
	    -ms-transform: scale(1) translateY(0);
	        transform: scale(1) translateY(0);
}
.create-content-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding-left: 15px;
	border-radius: 50px;
	background: #deeff5;
	-webkit-box-shadow: -1px 1px 5px 2px rgba(0, 0, 0, .1), -1px 1px 1px 0 rgb(255, 255, 255)inset;
	        box-shadow: -1px 1px 5px 2px rgba(0, 0, 0, .1), -1px 1px 1px 0 rgb(255, 255, 255)inset;
	z-index: 2;
}
.create-input {
	width: 100%;
	padding-right: 15px;
	color: #add8e6;
}
.create-button {
	-webkit-flex-shrink: 0;
	    -ms-flex-negative: 0;
	        flex-shrink: 0;
	width: 80px;
	height: 50px;
	border-radius: 50px;
	font-size: 16px;
	color: #88d4ec;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-shadow: -2px 0 2px 1px rgba(0, 0, 0, .1);
	        box-shadow: -2px 0 2px 1px rgba(0, 0, 0, .1);
}
.create-content::after {
	content: '';
	width: 20px;
	height: 20px;
	background: #deeff5;
	position: absolute;
	left: 0;
	right: 0;
	bottom: -10px;
	margin: 0 auto;
	-webkit-transform: rotate(45deg);
	    -ms-transform: rotate(45deg);
	        transform: rotate(45deg);
	-webkit-box-shadow: -1px 1px 5px 2px rgba(0, 0, 0, .1);
	        box-shadow: -1px 1px 5px 2px rgba(0, 0, 0, .1);
	z-index: -1;
}
.create-content-box::after {
	content: '';
	width: 20px;
	height: 20px;
	background: #deeff5;
	position: absolute;
	left: 0;
	right: 0;
	bottom: -10px;
	margin: 0 auto;
	-webkit-transform: rotate(45deg);
	    -ms-transform: rotate(45deg);
	        transform: rotate(45deg);
